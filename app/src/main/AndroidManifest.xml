<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".albums"
            android:label="@string/Music_album"/>
        <activity android:name=".artist"
            android:label="@string/Music_artist"/>
        <activity android:name=".year"
            android:label="@string/Music_year"/>
        <activity android:name=".genre"
            android:label="@string/Music_type"/>
    </application>

</manifest>